{
  "database": {
    "id": "event_logistics_db",
    "name": "Event Logistics Database",
    "backupPolicies": [
      {
        "name": "Daily Backup",
        "schedule": "0 0 * * *",
        "retention": 7,
        "enabled": true
      },
      {
        "name": "Weekly Backup",
        "schedule": "0 0 * * 0",
        "retention": 30,
        "enabled": true
      },
      {
        "name": "Monthly Backup",
        "schedule": "0 0 1 * *",
        "retention": 90,
        "enabled": true
      }
    ]
  }
}
